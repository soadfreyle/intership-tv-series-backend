{
    "name": "intership-tv-series-backend",
    "version": "0.0.1",
    "description": "this project has the functionality to search tv series",
    "main": "build/index.js",
    "scripts": {
        "start": "npm run clean && npm run build && node ./build/app.js",
        "start:dev": "nodemon --watch src -e ts,js --exec npm start",
        "debug": "npm run clean && npm run build && DEBUG=* node ./build/app.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc",
        "build:dev": "ts-node-dev --respawn --transpileOnly ./src/app.ts",
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "lint:fix": "npm run lint -- --fix",
        "clean": "rm -rf build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/soadfreyle/intership-tv-series-backend.git"
    },
    "author": "@soadfreyle",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/soadfreyle/intership-tv-series-backend/issues"
    },
    "homepage": "https://github.com/soadfreyle/intership-tv-series-backend#readme",
    "dependencies": {
        "axios": "^0.25.0",
        "debug": "^4.3.3",
        "express": "^4.17.2",
        "node-fetch-commonjs": "^3.1.1",
        "nodemon": "^2.0.15",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.5.5"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "tslint": "^6.1.3"
    }
}
